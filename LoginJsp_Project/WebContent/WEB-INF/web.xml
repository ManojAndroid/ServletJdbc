<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>LoginJsp_Project</display-name>
  <welcome-file-list>
    <welcome-file>Header.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>signup</servlet-name>
    <jsp-file>/Signup.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>form</servlet-name>
    <jsp-file>/LoginFormjsp.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>form</servlet-name>
    <url-pattern>/form</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/Login.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>webpage</servlet-name>
    <jsp-file>/Header.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>webpage</servlet-name>
    <url-pattern>/webpage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>landing</servlet-name>
    <jsp-file>/Header.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>landing</servlet-name>
    <url-pattern>/landing</url-pattern>
  </servlet-mapping>
</web-app>